{
  "name": "jbuilder",
  "version": "0.0.1",
  "description": "Building UIs with Designer tools",
  "productName": "JBuilder",
  "author": "",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev:api": "node server.js",
    "dev:web": "quasar dev",
    "dev": "concurrently -k -n api,web -c blue,green \"npm run dev:api\" \"npm run dev:web\"",
    "build": "quasar build",
    "postinstall": "quasar prepare"
  },
  "dependencies": {
    "@guolao/vue-monaco-editor": "^1.5.5",
    "@quasar/extras": "^1.16.4",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "quasar": "^2.16.0",
    "uuid": "^13.0.0",
    "vue": "3.5.20",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@quasar/app-vite": "^2.1.0",
    "@vue/eslint-config-prettier": "^10.1.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.4.0",
    "postcss": "^8.4.14",
    "prettier": "^3.3.3",
    "vite-plugin-checker": "^0.10.3",
    "vue-eslint-parser": "^10.2.0"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
